    "directory": "packages/eslint-config-y-base"
  },
  "bugs": {
    "url": "https://github.com/yuanhjty/scaffolds/issues"
  },
  "homepage": "https://github.com/yuanhjty/scaffolds#readme",
  "engines": {
    "node": "^8.10.0 || ^10.13.0 || >=11.10.1"
  },
  "peerDependencies": {
    "eslint": "^6.1.0",
    "eslint-plugin-import": "^2.18.2"
  }
