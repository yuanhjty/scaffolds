{
  "name": "eslint-config-y-base",
  "version": "4.0.1",
  "description": "eslint-config-y-base",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "lint": "eslint ./",
    "lint-fix": "eslint ./ --fix",
    "lint-staged": "lint-staged",
    "test": "tape test/*.js",
    "prepublishOnly": "npm run test"
  },
  "lint-staged": {
    "*.json": [
      "prettier --write",
      "git add"
    ],
    "*.js": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "keywords": [
    "eslint",
    "eslint config",
    "eslint react",
    "javascript"
  ],
  "author": "ShaoZhiyuan",
  "repository": {
    "type": "git",
